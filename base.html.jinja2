    function populateAudioGroup() {
        let num = document.getElementById("audio-group-selector").value;
        let inner_div = document.createElement("div");

        let utt = num;

        let table = document.createElement("table");
        table.className = "mdl-data-table";

        let headerRow = table.createTHead().insertRow();

        // Define your headers or use the appropriate values
        let headers = ["ID ", "Score wav", "Reference wav", "SMOS Score"];

        headers.forEach((header) => {
            let th = document.createElement("th");
            th.scope = "col";
            th.innerText = header;
            headerRow.appendChild(th);
        });
        console.log(rows);


        // 清除先前的音频控件
        
        rows.filter(wav => Number(wav[0]) === Number(num)).forEach(wav => {
            let row = table.insertRow();
            row.insertCell().innerText = num + '-' + wav[1];  // Assuming id is at index 0

            // Add audio controls for Score Audio
            let scoreAudioCell = row.insertCell();
            let scoreAudio = createAudio(wav[2]);
            scoreAudioCell.appendChild(scoreAudio);

            // Add audio controls for Reference Audio
            let referenceAudioCell = row.insertCell();
            let referenceAudio = createAudio(wav[3]);
            referenceAudioCell.appendChild(referenceAudio);

            // Input for Score
            let scoreInputCell = row.insertCell();
            let scoreInputContainer = document.createElement("div");
            scoreInputContainer.style.marginBottom = "10px";  // Set the margin-bottom property to 10 pixels

            let scoreInputLabel = document.createElement("label");
            scoreInputContainer.appendChild(scoreInputLabel);

            for (let i = 1; i <= 5; i++) {
            let scoreInput = document.createElement("input");
            scoreInput.type = "radio";
            scoreInput.value = i;
            scoreInput.id = (num-1)*4 + Number(wav[1]) + "-" + i;
            scoreInput.name = wav[2];
            if (i === 1) {
                scoreInput.checked = true;
            }

            let scoreInputLabel = document.createElement("label");
            scoreInputLabel.innerHTML = i;
            scoreInputLabel.setAttribute("for", scoreInput.id);

            scoreInputContainer.appendChild(scoreInput);
            scoreInputContainer.appendChild(scoreInputLabel);
            }

            scoreInputCell.appendChild(scoreInputContainer);
        });

      
        
        let uttTitle = document.createTextNode("Utterance " + utt);
        let uttTitleEl = document.createElement("h6");
        uttTitleEl.appendChild(uttTitle);

        inner_div.appendChild(uttTitleEl);
        inner_div.appendChild(table);

        
        var div = document.getElementById("audio-example");
        div.innerHTML = inner_div.innerHTML;

    }

    function createAudio(utterance) {

        const audio = document.createElement("audio");
        audio.controls = "controls";
        audio.preload = "none";
        audio.src = utterance;
        audio.innerText = "Your browser does not support the audio element.";
        return audio;
    }

    document.addEventListener("DOMContentLoaded", function() {
        populateAudioGroup();
    });



